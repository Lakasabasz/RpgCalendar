{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:8000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "http-nobrowser": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:8000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "FF_SENSITIVE_HEADERS": "true"
      }
    },
    "asdev-keycloak": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "launchUrl": "swagger",
      "applicationUrl": "http://localhost:8000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "KEYCLOAK_REALM": "rpgcalendar",
        "KEYCLOAK_INTERNAL_URL": "https://auth.dev.rpg-calendar.jakubkrawczyk.com",
        "KEYCLOAK_AUDIENCE": "account",
        "FF_KEYCLOAK_CERT": "true",
        "FF_SENSITIVE_HEADERS": "true"
      }
    }
  }
}
